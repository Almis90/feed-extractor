{
  "version": "1.0.02",
  "name": "feed-reader",
  "description": "Handle ATOM and RSS feed",
  "homepage": "https://www.npmjs.com/package/feed-reader",
  "repository": {
    "type": "git",
    "url": "https://github.com/ndaidong/feed-reader"
  },
  "author": "@ndaidong",
  "main": "./index.js",
  "engines": {
    "node": ">= 6.0"
  },
  "scripts": {
    "test": "./node_modules/.bin/snyk test && ./node_modules/.bin/tape test/start.js | tap-spec",
    "coverage": "./node_modules/.bin/nyc tape test/start.js | tap-spec",
    "report": "npm run coverage && ./node_modules/.bin/nyc report --reporter=lcov",
    "coveralls": "npm run report && cat ./coverage/lcov.info | ./node_modules/.bin/coveralls",
    "snyk": "./node_modules/.bin/snyk wizard"
  },
  "dependencies": {
    "bellajs": "^5.2.2",
    "friendly-xml": "^1.0.1",
    "html-entities": "^1.2.0",
    "node-fetch": "^1.5.3",
    "promise-wtf": "^0.1.25",
    "xml2json": "^0.9.1"
  },
  "devDependencies": {
    "coveralls": "latest",
    "nyc": "latest",
    "snyk": "latest",
    "tap-spec": "latest",
    "tape": "latest"
  },
  "keywords": [
    "extractor",
    "parser",
    "feed",
    "rss",
    "atom",
    "util"
  ],
  "license": "MIT",
  "maintainers": [
    "ndaidong <ndaidong@gmail.com>"
  ],
  "bugs": {
    "url": "https://github.com/ndaidong/feed-reader/issues"
  },
  "directories": {
    "test": "test"
  }
}
