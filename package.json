{
  "version": "1.1.4",
  "name": "feed-reader",
  "description": "Handle ATOM and RSS feed",
  "homepage": "https://www.npmjs.com/package/feed-reader",
  "repository": {
    "type": "git",
    "url": "https://github.com/ndaidong/feed-reader"
  },
