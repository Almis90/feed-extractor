{
  "version": "2.0.0",
  "name": "feed-reader",
  "description": "Extract ATOM/RSS content from given feed url",
  "homepage": "https://www.npmjs.com/package/feed-reader",
  "repository": {
    "type": "git",
    "url": "git@github.com:ndaidong/feed-reader.git"
  },
  "author": "@ndaidong",
  "module": "./src/main.js",
  "main": "./index.js",
  "engines": {
    "node": ">= 8.6"
  },
  "scripts": {
    "start": "DEBUG=feed-reader:* node index",
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "tap tests/start.js --coverage --reporter=spec",
    "eval": "DEBUG=feed-reader:* node -r esm tests/run",
    "reset": "node reset"
  },
  "dependencies": {
    "bellajs": "^8.0.2",
    "debug": "^4.1.1",
    "esm": "^3.2.25",
    "html-entities": "^1.3.1",
    "lru-cache": "^5.1.1",
    "node-fetch": "^2.6.0",
    "xml-js": "^1.6.11"
  },
  "devDependencies": {
    "eslint-config-goes": "^1.1.8",
    "nock": "^12.0.3",
    "tap": "^14.10.7"
  },
  "keywords": [
    "extractor",
    "parser",
    "feed",
    "rss",
    "atom",
    "util"
  ],
  "license": "MIT"
}
